#content{
  height: 100%;
  display:flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.avatar{
  width: 400rpx;
  height: 400rpx;
  border-radius: 50%;
}
.btn{
  padding: 10rpx;
  border: 5rpx solid rgb(47, 165, 106);
  border-radius: 20rpx;
  color: deepink;
}
.parent{
  border: 1px solid #000;
  width: 400rpx;
  height: 400rpx;
  text-align: center;
  line-height: 400rpx;
}
.child{
  border:1px solid #000;
 width: 500rpx;
